#include <iostream>
#include "math.h"
#include "conio.h"
#include "stdafx.h"

using namespace std;

void main()
{
   float x1,y1,x2,y2 D;
   cout<<"introducir x1 y y1"<<endl;
   cin>> x1,y1;
   cout<<"ingresar x2 y y2"<<endl;
   cin>> x2,y2;
   D=sqrt(pow(x2-x1, 2)+pow(y2-y1, 2));
   cout<<"es valor de D es"<<D<<endl;

   getch(); 
}
