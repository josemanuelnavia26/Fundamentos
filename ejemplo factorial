#include "stdafx.h"
#include <iostream>
#include "conio.h"


using namespace std;

int factorial(int n);




void main()
{
 int N;
 cout<<"ingrese el valor de N"<<endl;
 cin>>N;
 cout<<"el factorial del numero N es:"<< factorial(N)<<endl;
 getch();


}

int factorial(int n)

{

	int prod=1;
	for (int i=2;i<=n;i++)
	prod=prod*1;
	return prod;

}
