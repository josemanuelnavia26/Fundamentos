#include "stdafx.h"
#include "conio.h"
#include <iostream>

using namespace std;
int main()
{
	int numeros[] = {1,2,3,4,5}
	int suma = 0;
	int mult = 1;

	for(int i=0; i<5; i++)

	{
		suma += numeros[i];
		mult= mult*numeros[i];
	}


	cout<<"la suma de los elementos del vector es: "<<suma<<endl;
	cout<<"la multiplicacion es:"<<mult<<endl;

	


	getch();
	return 0;
}

